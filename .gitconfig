[user]
    name = Tony Grosinger
    email = tony@grosinger.net
[color]
    ui = auto
[alias]
  co = checkout
  c = commit
  s = status
  b = branch
  pp = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  type = cat-file -t
  dump = cat-file -p
